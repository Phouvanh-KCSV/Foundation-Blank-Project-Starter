{
  "name": "foundation-blank-project-starter",
  "version": "1.0",
  "authors": [
    "Phouvanh KCSV <Phouvanh.KCSV@gmail.com>"
  ],
  "license": "MIT",
  "install": {
    "path": "public/assets/components",
    "ignore": ["fastclick", "jquery.cookie", "jquery-placeholder", "modernizr"],
    "sources": {
      "jquery": "bower_components/jquery/dist/jquery.min.js",
      "foundation": [
        "bower_components/foundation/js/foundation.min.js"
      ],
      "components-font-awesome": {
        "mapping": [
          {"bower_components/components-font-awesome/css/font-awesome.min.css": "css/font-awesome.min.css"},
          {"bower_components/components-font-awesome/fonts/FontAwesome.otf": "fonts/FontAwesome.otf"},
          {"bower_components/components-font-awesome/fonts/fontawesome-webfont.eot": "fonts/fontawesome-webfont.eot"},
          {"bower_components/components-font-awesome/fonts/fontawesome-webfont.svg": "fonts/fontawesome-webfont.svg"},
          {"bower_components/components-font-awesome/fonts/fontawesome-webfont.ttf": "fonts/fontawesome-webfont.ttf"},
          {"bower_components/components-font-awesome/fonts/fontawesome-webfont.woff": "fonts/fontawesome-webfont.woff"},
          {"bower_components/components-font-awesome/fonts/fontawesome-webfont.woff2": "fonts/fontawesome-webfont.woff2"}
        ]
      }
    }
  }
}
